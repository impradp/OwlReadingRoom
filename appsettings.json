{
  "Auth0": {
    "Domain": "${AUTH0_DOMAIN}",
    "ClientId": "${AUTH0_CLIENT_ID}",
    "Scope": "openid profile email",
    "Audience": "${AUTH0_AUDIENCE}",
    "RedirectUri": "${AUTH0_REDIRECT_URI}",
    "PostLogoutRedirectUri": "${AUTH0_POST_LOGOUT_REDIRECT_URI}"
  },
  "SmtpSettings": {
    "Server": "${SMTP_SERVER}",
    "Port": "${SMTP_PORT}",
    "SenderName": "${SMTP_SENDER_NAME}",
    "Username": "${SMTP_USERNAME}",
    "Password": "${SMTP_PASSWORD}",
    "Receiver": "${SMTP_RECEIVER}",
    "ReceiverName": "${SMTP_RECEIVER_NAME}",
    "UseSsl": "${SMTP_USE_SSL}"
  },
  "Features": {
    "RoomActions": {
      "Delete": "${FEATURE_ROOM_ACTIONS_DELETE}",
      "Edit": "${FEATURE_ROOM_ACTIONS_EDIT}",
      "View": "${FEATURE_ROOM_ACTIONS_VIEW}"
    },
    "PackageActions": {
      "Delete": "${FEATURE_PACKAGE_ACTIONS_DELETE}",
      "Edit": "${FEATURE_PACKAGE_ACTIONS_EDIT}",
      "View": "${FEATURE_PACKAGE_ACTIONS_VIEW}"
    }
  },
  "Company": {
    "Name": "${COMPANY_NAME}",
    "Address": "${COMPANY_ADDRESS}",
    "City": "${COMPANY_CITY}",
    "MobileNo": "${COMPANY_MOBILE_NO}",
    "AlternateMobileNo": "${COMPANY_ALTERNATE_MOBILE_NO}",
    "EmailID": "${COMPANY_EMAIL_ID}",
    "AlternateEmailID": "${COMPANY_ALTERNATE_EMAIL_ID}"
  }
}
